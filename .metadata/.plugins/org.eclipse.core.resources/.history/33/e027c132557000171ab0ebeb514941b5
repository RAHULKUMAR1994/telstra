import java.util.Dictionary;
import java.util.Enumeration;
import java.util.HashMap;
import java.util.Hashtable;
import java.util.LinkedList;


public class dictionary {

	public static void main(String arfs[])
	{
		
		 Dictionary<Integer, Integer> dict = new Hashtable<Integer, Integer>();
		 
		 LinkedList<String> lin = new LinkedList<String>();
		 
		 lin.add("hello");
		 lin.add("world");
		 lin.add("hi");
		 lin.add("bro");
		 
		int si = lin.size()	;	 
		 while(si > 0)
		 {
			 System.out.println(lin.indexOf(--si));
		 }
		 
		 dict.put(1,1);
		 dict.put(2,3);
		 
		 Enumeration<Integer> enum1 =  dict.keys();
		
		 int size = dict.size();
		 
		// System.out.println(dict.get(enum1.nextElement()));
		 
		 
		 
		 while(size-- > 0)
		 {
			 System.out.println(dict.get(enum1.nextElement()));
		 }
		 
		
	}
	
}
